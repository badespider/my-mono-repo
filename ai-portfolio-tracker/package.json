{
  "name": "ai-portfolio-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:unit": "vitest run --config vitest.config.ts src/**/*.unit.test.{ts,tsx}",
    "test:integration": "vitest run --config vitest.config.ts src/**/*.integration.test.{ts,tsx}",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "deploy:vercel": "vercel --prod",
    "deploy:vercel:preview": "vercel",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:netlify:preview": "netlify deploy",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:docker": "docker build -t ai-portfolio-tracker .",
    "docker:run": "docker run -p 3000:3000 ai-portfolio-tracker",
    "env:check": "node -e \"console.log('Environment check passed')\""
  },
  "dependencies": {
    "@chakra-ui/react": "^3.22.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.2",
    "@types/yup": "^0.29.14",
    "axios": "^1.11.0",
    "chart.js": "^4.4.4",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.23.7",
    "next": "15.4.3",
    "react": "19.1.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "socket.io-client": "^4.8.1",
    "swr": "^2.3.4",
    "yup": "^1.6.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/socket.io-client": "^1.4.36",
    "@types/ws": "^8.5.14",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "msw": "^2.8.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "ws": "^8.18.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
